//1
plubic class Funcao01
{

    static int caixaPreta(int a, int) {
           return a*a+b;
     }
    public static void main(String []args){

           int resposta;
           resposta = caixaPreta(3, 5);
          system.out.print(resposta);
    }
}

//2.
public class Funcao02
{ 
    static double caulcular(double a, double b) {
          double x;

        x = a*a + 2*a*b + b*b;
        return x;
}

public static void main(String []args) {

       double resposta;
       resposta = Calcular(2,3);
       system.out.println(resposta);
      }
}












